module draw_square (
    input logic [10:0] pix_x,
    input logic [10:0] pix_y,
    input logic [11:0] sw,
    output logic [11:0] rgb_out
);

always_comb begin
    if ((pix_y >= 11'd250) && (pix_y <= 11'd300) 
	 && (pix_x >= 11'd400) && (pix_x <= 11'd500)) begin
        rgb_out = sw;
    end else begin
        rgb_out = ~sw;
    end
end

endmodule